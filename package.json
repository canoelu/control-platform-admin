{
  "name": "control-platform-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run start",
    "start": "npm run serve:watch",
    "serve": "vue-cli-service serve --mode development",
    "lint": "vue-cli-service lint",
    "serve:watch": "concurrently \"npm run watch\" \"npm run serve\"",
    "watch": "node ./watch.js",
    "generate:route": "node route-generator.js",
    "build": "npm run generate:route && vue-cli-service build --mode prod",
    "build:dev": "npm run generate:route && vue-cli-service build --mode develop",
    "build:test": "npm run generate:route && vue-cli-service build --mode test",
    "build:prod": "npm run generate:route && vue-cli-service build --mode prod",
    "prettier": "prettier --loglevel silent --write src/**/*.vue src/**/*.js src/**/*.ts src/**/*.scss",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.0.2",
    "core-js": "^3.4.3",
    "dayjs": "^1.8.17",
    "element-ui": "^2.13.0",
    "lodash": "^4.17.15",
    "nprogress": "^0.2.0",
    "shelljs": "^0.8.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-cookies": "^1.6.1",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "vuex-class": "^0.3.2",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-e2e-cypress": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-unit-mocha": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  }
}
